{
  "expo": {
    "name": "Trilha das Árvores",
    "slug": "app",
    "version": "1.0.0",
    "android": {
      "package": "trilha.android",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA6zpa6WdxJ9FwGnpSuPqs745DZaGVOZiQ"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.INTERNET",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "ios": {
      "bundleIdentifier": "trilha.ios",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Precisamos da sua localização para calcular a distância das árvores.",
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera"
      }
    },
    "plugins": [
      "expo-font",
      "expo-location",
      [
        "react-native-vision-camera",
        {
          "enableCodeScanner": true,
          "cameraPermissionText": "$(PRODUCT_NAME) needs access to your Camera."
        }
      ]
      
      
    ],
    "extra": {
      "eas": {
        "projectId": "773733a3-33d4-47ce-a9ef-54ac5a1f8d0c"
      }
    }
  }
}